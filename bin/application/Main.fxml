<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MyController">
   <children>
      <Label fx:id="status" alignment="CENTER" layoutX="90.0" layoutY="371.0" prefHeight="40.0" prefWidth="220.0" text="Not Connected to any PC">
         <font>
            <Font name="Cambria" size="14.0" />
         </font>
      </Label>
      <Label fx:id="data" alignment="CENTER" layoutX="90.0" layoutY="284.0" prefHeight="68.0" prefWidth="220.0" text="Process" textFill="#a31a1a">
         <font>
            <Font name="Cambria" size="14.0" />
         </font>
      </Label>
      <Button layoutX="35.0" layoutY="172.0" mnemonicParsing="false" onAction="#stop" prefHeight="40.0" prefWidth="110.0" text="Start Robot">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="259.0" layoutY="172.0" mnemonicParsing="false" onAction="#takeScreenShot" prefHeight="40.0" prefWidth="110.0" text="Screen-shot">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="175.0" layoutY="446.0" mnemonicParsing="false" onAction="#close" text="Close">
         <font>
            <Font name="Cambria Bold" size="13.0" />
